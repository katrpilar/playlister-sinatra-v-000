  <form action="/songs" method="POST">

    <label> Artist Name </label>
    <input type="text" name="Artist Name"> </br>

    <label>Name</label>
      <input type="text" name="Name" id="Name">

      <br></br>

    <label>Select an Genre
      <%Genre.all.each do |genre|%>
        <label><%=genre.name%>
          <input type="checkbox" name="<%=genre.slug%>" value="<%=genre.slug%>" id="<%=genre.slug%>">
        </label>
      <%end%>
    </label>

    <br></br>

    <label>New Genre Name
       <input  type="text" name="genre_name" id="genre_name">
     </label>

    <input type="submit" value="Create">
  </form>
